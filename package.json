{
  "name": "cv-automater",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.ts",
  "scripts": {
    "vite": "vite",
    "build-electron": "tsc electron/preload.ts --outDir electron --esModuleInterop --skipLibCheck --target ES2020 --module CommonJS",
    "electron:start": "wait-on http://localhost:5173 && electron .",
    "dev": "concurrently \"npm run build-electron && npm run vite\" \"npm run electron:start\""
  },
  "dependencies": {
    "dayjs": "^1.11.18",
    "electron-clipboard-watcher": "^1.0.1",
    "exceljs": "^4.4.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^38.4.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "wait-on": "^9.0.1"
  }
}
